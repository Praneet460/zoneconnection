<% include partials/header %>

<div class="container" style="padding-top:100px;">
  <div class="panel panel-default" >
    <!-- Default panel contents -->
    <div class="panel-heading">Panel heading</div>

    <!-- Table -->
    <table class="table">
      <% company.forEach(function(company){ %>
      <tr>
          <td><%= company.company %></td>
          <td><%= company.country %></td>
          <td><a href="<%= company.facebook %>" target="_blank"><i class="fa fa-facebook-square" style="font-size:24px"></i></a><br><kbd>accenture</kbd></td>
          <td><a href="<%= company.twitter %>" target="_blank"><i class="fa fa-twitter-square" style="font-size:24px"></i></a><br><kbd>Accenture</kbd></td>
        </tr>
        <% }); %>
    </table>
  </div>
</div>


<% include partials/footer %>
